
<div class="right-wrap slidein" id="slidePanel">
	<h2>{{#link-to "people" class="btn btn-default right-close"}}<i class="glyphicon glyphicon-chevron-right"></i>{{/link-to}} Person</h2>
	<table>
		<tr>
			<th>First Name</th>
			<td>{{firstName}}</td>
		</tr>
		<tr>
			<th>Last Name</th>
			<td>{{lastName}}</td>
		</tr>
		<tr>
			<th>Company Name</th>
			<td>{{companyName}}</td>
		</tr>
		<tr>
			<th>Label</th>
			<td>{{label}}</td>
		</tr>
		<tr>
			<th>Address</th>
			<td>{{address}}</td>
		</tr>
		<tr>
			<th>City</th>
			<td>{{city}}</td>
		</tr>
		<tr>
			<th>State</th>
			<td>{{state}}</td>
		</tr>
		<tr>
			<th>Phone</th>
			<td>{{phone}}</td>
		</tr>
		<tr>
			<th>Mobile</th>
			<td>{{mobile}}</td>
		</tr>
		<tr>
			<th>Fax</th>
			<td>{{fax}}</td>
		</tr>
		<tr>
			<th>Email</th>
			<td>{{email}}</td>
		</tr>
		<tr>
			<th>Notes</th>
			<td>{{notes}}</td>
		</tr>
	</table>
</div>
