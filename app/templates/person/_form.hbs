<table>
	<tr>
		<th>First Name</th>
		<td>{{input value=firstName}}</td>
	</tr>
	<tr>
		<th>Last Name</th>
		<td>{{input value=lastName}}</td>
	</tr>
	<tr>
		<th>Company Name</th>
		<td>{{input value=companyName}}</td>
	</tr>
	<tr>
		<th>Label</th>
		<td>{{label}}</td>
	</tr>
	<tr>
		<th>Address</th>
		<td>{{input value=address}}</td>
	</tr>
	<tr>
		<th>City</th>
		<td>{{input value=city}}</td>
	</tr>
	<tr>
		<th>State</th>
		<td>{{input value=state}}</td>
	</tr>
	<tr>
		<th>Phone</th>
		<td>{{input value=phone}}</td>
	</tr>
	<tr>
		<th>Mobile</th>
		<td>{{input value=mobile}}</td>
	</tr>
	<tr>
		<th>Fax</th>
		<td>{{input value=fax}}</td>
	</tr>
	<tr>
		<th>Email</th>
		<td>{{input value=email}}</td>
	</tr>
	<tr>
		<th>Notes</th>
		<td>{{textarea value=notes}}</td>
	</tr>
</table>
